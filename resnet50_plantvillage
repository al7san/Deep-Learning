 in model.fc.parameters():\n    p.requires_grad = True\n\noptimizer_ft = optim.Adam(filter(lambda p: p.requires_grad, model.parameters()), lr=1e-4, weight_decay=1e-4)\nscheduler = optim.lr_scheduler.StepLR(optimizer_ft, step_size=10, gamma=0.5)\n\nearly2 = EarlyStopping(patience=5)\n\nMAX_EPOCHS = 50\nfor epoch in range(1, MAX_EPOCHS + 1):\n    tr_loss, tr_acc = run_epoch(model, train_loader, optimizer=optimizer_ft)\n    va_loss, va_acc = run_epoch(model, val_loader, optimizer=None)\n    scheduler.step()\n\n    print(f\"FT Epoch {epoch}/{MAX_EPOCHS} | Train: loss={tr_loss:.4f}, acc={tr_acc:.4f} | Val: loss={va_loss:.4f}, acc={va_acc:.4f} | LR={scheduler.get_last_lr()[0]:.6f}\")\n    save_best_checkpoint(model, va_loss, epoch, base_dataset.classes, phase_name=\"Phase2\")\n\n    if early2.step(va_loss, model):\n        print(\"Early stopping (fine-tune) \")\n        break\n\nif early2.best_state is not None:\n    model.load_state_dict(early2.best_state)\n","metadata":{"id":"u_n8gaHFUC3Z","trusted":true,"execution":{"iopub.status.busy":"2025-12-20T07:08:28.166536Z","iopub.execute_input":"2025-12-20T07:08:28.166914Z","iopub.status.idle":"2025-12-20T08:28:10.316351Z","shell.execute_reply.started":"2025-12-20T07:08:28.166884Z","shell.execute_reply":"2025-12-20T08:28:10.315506Z"}},"outputs":[{"name":"stderr","text":"                                                 \r","output_type":"stream"},{"name":"stdout","text":"FT Epoch 1/50 | Train: loss=0.1432, acc=0.9550 | Val: loss=0.0575, acc=0.9821 | LR=0.000100\n✅ Saved BEST checkpoint (Phase2) @ epoch 1 | val_loss=0.0575\n","output_type":"stream"},{"name":"stderr","text":"                                                 \r","output_type":"stream"},{"name":"stdout","text":"FT Epoch 3/50 | Train: loss=0.0548, acc=0.9820 | Val: loss=0.0322, acc=0.9886 | LR=0.000100\n✅ Saved BEST checkpoint (Phase2) @ epoch 3 | val_loss=0.0322\n","output_type":"stream"},{"name":"stderr","text":"                                                 \r",